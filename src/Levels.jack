        let strArray[6] =  "#C                          #";
        let strArray[7] =  "#C                          #";
        let strArray[8] =  "#C  C                       #";
        let strArray[9] =  "#C  #                       #";
        let strArray[10] = "#C                         ##";
        let strArray[11] = "#C   CC#                    #";
        let strArray[12] = "#############################";
        

        let level = Level.parse(1, strArray, 29, 13);
        do strArray.dispose();
        return level;
    }

