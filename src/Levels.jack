class Levels {
    
    function Level loadLevel(int id) {
        if (id = 0) {
            return Levels.first();
        }
        if (id = 1) {
            return Levels.medium();
        }
        if (id = 2) {
            return Levels.hard();
        }
        do Main.waitForContinue("YOU WIN, IDI C# delay!!!");
        do Sys.halt();
        return 0;
    }

    function Level first() {
        var Array strArray;
        var Level level;

        let strArray = Array.new(13); // maxHEIGHT = 13, maxWidth = 29
        let strArray[0] =  "#W###########################";
        let strArray[1] =  "#P   #F#                    #";
        let strArray[2] =  "#C#  #L#                   ##";
        let strArray[3] =  "#K                          #";
