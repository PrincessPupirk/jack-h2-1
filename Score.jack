class Score {
    field int coins;
    field int keys;

    constructor Score new() {
        let coins = 0;
        let keys = 0;
        return this;
    }

    method void IncrementCoins() {
        let coins = coins + 1;
        return;
    }

    method void IncrementKeys() {
        let keys = keys + 1;
        return;
    }

    method void dispose() {
        do Memory.deAlloc(this);
        return;
    }
}
